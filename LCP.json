{
  "id": "ui",
  "loadBalancer": {
    "targetPort": 80
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/",
      "port": 80
    }
  },
  "memory": 512,
  "cpu": 2,
  "volumes": {
    "backup": "/home/storage1",
    "storage2": "/opt/storage2"
  }
}
